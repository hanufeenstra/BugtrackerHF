@model IUserViewModel
<!DOCTYPE html>
<html lang="en">
    <head>
    @await Html.PartialAsync("_MetaPartial")
        <title>@ViewBag.Title</title>
        @await RenderSectionAsync("Styles", required: true)
        <!-- Layout styles -->
        <link rel="stylesheet" href= "~/assets/css/style.css"> 
        <!-- End layout styles -->
    </head>
    <body>
    <div class="container-scroller">
        @await Html.PartialAsync("_SidebarPartial", Model)
        <div class="container-fluid page-body-wrapper">
            @await Html.PartialAsync("_NavbarPartial", Model)
            <div class="main-panel">
                @RenderBody()
                @await Html.PartialAsync("_FooterPartial")
            </div>
        </div>
    </div>
        @await RenderSectionAsync("Scripts", required: true)
    @await Html.PartialAsync("_ValidationScriptPartial")
    </body>
</html>