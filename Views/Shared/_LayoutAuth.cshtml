<!DOCTYPE html>
<html lang="en">
<head>
    @await RenderSectionAsync("Meta", required: true)
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href= "~/assets/css/style.css">
    @await RenderSectionAsync("Styles", required: true)    
</head>
<body>
<div class="container-scroller">
    @await RenderSectionAsync("Body", required: true)
</div>
@await RenderSectionAsync("Scripts", required: true)
@await RenderSectionAsync("Validation", required: true)
</body>
</html>


